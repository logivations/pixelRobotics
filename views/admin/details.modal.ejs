<div class="modal-dialog modal-lg">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel">User IP</h5>
            <button
                    type="button"
                    class="btn-close"
                    data-bs-dismiss="modal"
                    aria-label="Close">
            </button>
        </div>
        <div class="modal-body">
            <p><span class="fw-bolder">IP:</span> <%= detailedIP.query %></p>
            <p><span class="fw-bolder">City:</span> <%= detailedIP.city %></p>
            <p><span class="fw-bolder">Country:</span> <%= detailedIP.country %></p>
            <p><span class="fw-bolder">User Agent:</span> <%= detailedIP.userAgent %></p>
            <iframe
                width="100%"
                height="300px"
                id="gmap_canvas"
                src="https://maps.google.com/maps?q=<%= detailedIP.lat %>,<%= detailedIP.lon %>&hl=es&z=14&amp;output=embed">
            </iframe>

        </div>
        <div class="modal-footer">
            <button type="button" class="btn-custom" data-bs-dismiss="modal">Close</button>
        </div>
    </div>
</div>
